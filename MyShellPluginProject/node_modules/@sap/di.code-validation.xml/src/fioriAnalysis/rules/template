/* eslint-disable no-console, no-undef */
define([], function() {
    "use strict";
/* eslint-enable no-undef*/

    return {
        id: "",
        category: "Error",
        path: "",
        errorMsg: "",
        validate: function(report, path, nodes){
            var result = [];
            /* eslint-disable no-undef */
            console.log("validating (" + this.id + ")");
            /* eslint-enable no-undef*/
            var node = nodes.iterateNext();
            while (node) {
                if(node){
                    result.push(node);
                }
                node = nodes.iterateNext();
            }
            return result;
        }
    };
});
