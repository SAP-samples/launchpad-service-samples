{
	"title": "SAP JSON schema for Web Application Manifest File",
		
	"$schema": "http://json-schema.org/draft-04/schema#",
		
	"type": "object",
	
	"required": ["_version", "sap.app", "sap.ui"],

  	"additionalProperties": false,

	"properties": {
		
		"_version": {
			"description": "Represents Application Descriptor format version. It is managed by schema owner",
			"type": "string",
			"enum" :["1.1.0", "1.2.0", "1.3.0","1.4.0","1.5.0", "1.6.0", "1.7.0", "1.8.0", "1.9.0", "1.10.0", "1.11.0", "1.12.0"]
		},

		"start_url":  {
			"description": "Represents the URL that the developer would prefer the user agent load when the user launches the web application",
			"type": "string"
		},
	  	"sap.app": {
			"oneOf": [
                {
                    "$ref": "sap.app/1.1.0/subschema_sapapp.json"
                },
			  {
				"$ref": "sap.app/1.2.0/subschema_sapapp.json"
			  },
			  {
				"$ref": "sap.app/1.3.0/subschema_sapapp.json"
			  }
            ]
		},
	  	"sap.ui": {
			"oneOf": [
                {
                    "$ref": "sap.ui/1.1.0/subschema_sapui.json"
                },
			  {
				"$ref": "sap.ui/1.2.0/subschema_sapui.json"
			  },
			  {
				"$ref": "sap.ui/1.3.0/subschema_sapui.json"
			  }
            ]
		},
		
	  	"sap.ui5": {
			"oneOf": [
                {
                    "$ref": "sap.ui5/1.1.0/subschema_sapui5.json"
                },
			  {
				"$ref": "sap.ui5/1.2.0/subschema_sapui5.json"
			  }

            ]
		},
				
	 	 "sap.platform.abap": {
			"oneOf": [
                {
                    "$ref": "sap.platform.abap/1.1.0/subschema_sapplatformabap.json"
                },
			  {
				"$ref": "sap.platform.abap/1.2.0/subschema_sapplatformabap.json"
			  }

            ]
		},
				
	  	"sap.platform.hcp": {
			"oneOf": [
                {
                    "$ref": "sap.platform.hcp/1.1.0/subschema_sapplatformhcp.json"
                },
			  {
				"$ref": "sap.platform.hcp/1.2.0/subschema_sapplatformhcp.json"
			  }

            ]
		},
				
	  	"sap.fiori": {
			"oneOf": [
                {
                    "$ref": "sap.fiori/1.1.0/subschema_sapfiori.json"
                }
            ]
		},
				
	  	"sap.ui.generic.app": {
			"oneOf": [
                {
                    "$ref": "sap.ui.generic.app/1.1.0/subschema_sapuigenericapp.json"
                }
            ]
		},

	  "sap.flp": {
		"oneOf": [
		  {
			"$ref": "sap.flp/1.1.0/subschema_sapflp.json"
		  }
		]
	  },

	  "sap.ovp": {
		"oneOf": [
		  {
			"$ref": "sap.ovp/1.1.0/subschema_sapovp.json"
		  }
		]
	  },

	  "sap.wda": {
		"oneOf": [
		  {
			"$ref": "sap.wda/1.1.0/subschema_sapwda.json"
		  }
		]
	  },

	  "sap.apf": {
		"oneOf": [
		  {
			"$ref": "sap.apf/1.1.0/subschema_sapapf.json"
		  }
		]
	  },

	  "sap.cloud.portal": {
		"oneOf": [
		  {
			"$ref": "sap.cloud.portal/1.1.0/subschema_sapcloudportal.json"
		  }
		]
	  },

	  "sap.gui": {
		"oneOf": [
		  {
			"$ref": "sap.gui/1.1.0/subschema_sapgui.json"
		  }
		]
	  },
				
	  	"sap.ui.smartbusiness.app": {
			"oneOf": [
                {
                    "$ref": "sap.ui.smartbusiness.app/1.1.0/subschema_sapuismartbusinessapp.json"
                }
            ]
		},
				
	  	"sap.mobile": {
			"oneOf": [
                {
                    "$ref": "sap.mobile/1.1.0/subschema_sapmobile.json"
                }
            ]
		}
	}
}